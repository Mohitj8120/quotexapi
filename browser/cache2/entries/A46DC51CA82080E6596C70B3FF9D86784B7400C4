// misc definitions
var NOPROXY = "DIRECT";
var RETPROXY = NOPROXY;

// set this to 1 in order to enable debug output to the Java console
var DEBUG = 0;

function isInDomain(host, domain) {
  // fix . prefix problem of the original function
  if (host.length > domain.length) {
    return (host.substring(host.length - domain.length - 1) == "." + domain);
  }
  return (host == domain);
}

/* === end of initial setup ================================================ */

